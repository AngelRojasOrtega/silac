	<parameter name="mes" class="java.lang.String"/>
	<parameter name="anio" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT m.fecha as fecha, c.categoria as categoria, a.nombreArticulo, IFNULL(SUM(s.cantidad),0) as cantidad, e.precio as precio, (CEIL((IFNULL(SUM(s.cantidad),0)*precio)*100)/100) as total 
                FROM tblcatalogo c 
                INNER JOIN tblarticulo a ON a.idCatalogo = c.idCatalogo 
                INNER JOIN tblmovimiento m ON m.idArticulo = a.idArticulo 
                INNER JOIN tblsalida s ON s.idMovimiento = m.idMovimiento 
                INNER JOIN tblentrada e ON e.idEntrada = s.idEntrada 
WHERE MONTH(m.fecha) = $P{mes} AND YEAR(m.fecha) = $P{anio}
GROUP BY c.idCatalogo, a.idArticulo, e.precio]]>
	</queryString>
